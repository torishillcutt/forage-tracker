<h1>Edit this Expedition!</h1>
<br>
<%= render partial: 'layouts/error', locals: {object: @expedition} %>

<%= form_with scope: :expedition, url: user_expedition_path(@user), method: :patch do |f| %>
<%= render partial: 'form', locals: {f: f, user: @user, items: @user.items, new_items: @new_items, harvests: @harvests} %>

<% end%>