<%= render "/layouts/error", object: @user %>
<% if flash[:error] %>
<%= flash[:error] %>
<% end %>
    <%= form_for(@user, :url => sessions_path, :method => "post") do |f| %>
      <%= f.label :username, "Username or Email", :class => "form-label" %>
      <%= f.text_field :username, :class => "form-field" %>
      <%= f.label :password, "Password", :class => "form-label" %>
      <%= f.password_field :password, :class => "form-field" %><br>
      <%= f.submit "Log in", :class => "form-submit-btn" %><br>
      <%= button_to "Log in with Google", "/google_signup", :class => "btn btn-primary" %><br><br>
      <%= link_to "Sign up",  new_user_path %>
